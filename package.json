{
  "name": "twiggotronix",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "create-entities-index": "ts-node --pretty --project=tools/tsconfig.tools.json tools/create-entities-index.ts",
    "ts-node": "./node_modules/.bin/ts-node",
    "mmt-api:ts-node": "yarn ts-node --project=apps/mmt-api/tsconfig.app.json",
    "mmt-api:typeorm": "yarn mmt-api:ts-node ./node_modules/.bin/typeorm --config apps/mmt-api/src/ormconfig.ts",
    "mmt-api:migrate:generate": "yarn mmt-api:typeorm migration:generate -n",
    "mmt-api:migrate:run": "yarn mmt-api:typeorm migration:run",
    "mmt-api:migrate:revert": ".yarn mmt-api:typeorm migration:revert"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "fast-glob": "^3.2.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "sqlite3": "^5.0.2",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.8.3",
    "@nrwl/eslint-plugin-nx": "13.8.3",
    "@nrwl/jest": "13.8.3",
    "@nrwl/linter": "13.8.3",
    "@nrwl/nest": "^13.8.3",
    "@nrwl/node": "13.8.3",
    "@nrwl/tao": "13.8.3",
    "@nrwl/workspace": "13.8.3",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  }
}
